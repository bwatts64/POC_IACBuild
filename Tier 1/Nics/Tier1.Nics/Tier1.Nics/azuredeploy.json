{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "Frontend1-nicName": {
      "type": "string",
      "minLength": 1
    },
    "virtualNetworkId": {
      "type": "string"
    },
    "dmzSubnetId": {
      "type": "string"
    },
    "internalSubnetId": {
      "type": "string"
    },
    "clusterSubnetId": {
      "type": "string"
    }
  },
  "variables": {
    },
  "resources": [
    {
      "name": "[parameters('Frontend1-nicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "eastus",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "Frontend1-nic"
      },
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAllocationMethod": "Dynamic",
              "subnet": {
                "id": "[parameters('dmzSubnetId')]"
              }
            }
          }
        ]
      }
    }
  ],
  "outputs": {
  }
}
